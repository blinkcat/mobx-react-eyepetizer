		"react-transition-group": "1.x"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.77",
		"@types/react": "^16.0.10",
		"@types/react-dom": "^16.0.1",
		"@types/react-router-dom": "^4.0.8",
		"@types/webpack-env": "^1.13.2",
		"add": "^2.0.6",
		"awesome-typescript-loader": "^3.2.3",
