{
	"name": "mobx-react-eyepetizer",
	"version": "0.1.0",
	"description": "react eyepetizer",
	"scripts": {
		"predev": "rm -rf dist && tsc",
		"dev": "concurrently \"tsc -w\" \"next dist\"",
		"start": "next start dist",
		"test": "jest ./src",
		"coverage": "jest ./src --coverage",
		"prebuild": "rm -rf dist && tsc && cp -f next.config.js ./dist",
		"build": "next build dist && next export dist",
		"gh-pages": "touch dist/out/.nojekyll",
		"analyze": "cross-env ANALYZE=1 yarn build"
	},
	"dependencies": {
		"axios": "^0.17.1",
		"classnames": "^2.2.5",
		"fbjs": "^0.8.16",
		"hoist-non-react-statics": "^2.3.1",
		"lodash.curryright": "^4.1.1",
		"lodash.foreach": "^4.5.0",
		"lodash.get": "^4.4.2",
		"lodash.padstart": "^4.6.1",
		"lodash.pick": "^4.4.0",
		"lodash.set": "^4.3.2",
		"qs": "^6.5.1",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-transition-group": "^2.2.1"
	},
	"devDependencies": {
		"@types/jest": "^21.1.8",
		"@types/lodash": "^4.14.86",
		"@types/nock": "^8.2.1",
		"@types/react": "^16.0.25",
		"@types/react-dom": "^16.0.3",
		"@types/react-test-renderer": "^16.0.0",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"babel-plugin-transform-define": "^1.3.0",
		"concurrently": "^3.5.1",
		"coveralls": "^3.0.0",
		"cross-env": "^5.0.5",
		"jest": "^21.2.1",
		"next": "^4.2.0",
		"nock": "^9.1.3",
		"node-sass": "^4.7.2",
		"react-test-renderer": "^16.2.0",
		"styled-jsx-plugin-sass": "^0.2.2",
		"ts-jest": "^21.2.3",
		"tslint": "^5.8.0",
		"typescript": "^2.6.2",
		"webpack-bundle-analyzer": "^2.9.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/blinkcat/mobx-react-eyepetizer.git"
	},
	"keywords": ["mobx", "react"],
	"author": "blinkcat",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/blinkcat/mobx-react-eyepetizer/issues"
	},
	"homepage": "https://github.com/blinkcat/mobx-react-eyepetizer#readme"
}
