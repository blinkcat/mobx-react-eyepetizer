{
	"name": "mobx-react-eyepetizer",
	"version": "0.1.0",
	"description": "mobx react eyepetizer",
	"main": "./src/index.js",
	"scripts": {
		"test": "jest --verbose",
		"start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/dev.js --hot",
		"build": "cross-env NODE_ENV=production webpack --color --progress --config ./config/prod.js"
	},
	"dependencies": {
